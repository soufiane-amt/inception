server {

listen   443;

ssl    on;
ssl_certificate    /home/www/openssl/server.crt;
ssl_certificate_key  /home/www/openssl/server.key;

server_name samajat.42.ma;
root   /var/www/html/;

location / {
    index index.html;
}

}

 
